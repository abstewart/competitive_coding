class Wheel() {
 public:
  int x;
  int y;
  int r;
  Wheel(int a, int b, int c) {
    x = a;
    y = b;
    r = c;
  }
}

bool isTouching(x1, y1, r1, x2, y2, r2) {
  return false;
}

void populateWheels(vector<Wheel> w) {
  int w;
  int a, b, c;
  cin >> w;
  for (int i = 0; i<w; i++) {
    cin >> a >> b >> c;
    Wheel tmp = new Wheel(a, b, c);
    vector.push(tmp);
  }
}

